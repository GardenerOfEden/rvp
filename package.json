{
  "name": "rvp",
  "version": "0.0.4",
  "private": true,
  "description": "Research Video Prototype",
  "author": "Studio Process <contact@process.studio>",
  "repository": {
    "type": "git",
    "url": "https://github.com/StudioProcess/rvp.git"
  },
  "license": "MPL-2.0",
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "chrome": "open -a 'google chrome' http://localhost:4200",
    "link": "npm link angular-cli",
    "postinstall": "typings install",
    "reinstall": "rm -rf node_modules dist tmp typings && npm install",
    "update": "git pull && npm prune && npm install",
    "typings": "typings",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "tape": "live-server dist/ --open='index.spec.html?myspec' --browser='google chrome'",
    "build": "rm -rf dist && ng build -prod && open dist"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
