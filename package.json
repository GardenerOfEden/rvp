    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@ngrx/core": "^1.0.1",
    "@ngrx/store": "^2.0.1",
    "ajv": "^4.1.5",
    "docuri": "^4.2.1",
    "es6-shim": "0.35.1",
    "file-saver": "^1.3.2",
    "foundation-sites": "^6.2.3",
    "jszip": "^3.0.0",
    "localforage": "^1.4.2",
    "lodash": "^4.13.1",
    "loglevel": "^1.4.1",
    "moment": "^2.14.1",
    "node-uuid": "^1.4.7",
    "pace-progress": "^1.0.2",
    "pouchdb": "^5.4.0",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "video.js": "^5.13.0",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular-cli": "1.0.0-beta.10",
    "codelyzer": "0.0.20",
    "ember-cli-inject-live-reload": "1.4.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "0.13.22",
    "karma-chrome-launcher": "0.2.3",
    "karma-jasmine": "0.3.8",
    "live-server": "^1.0.0",
    "node-sass": "3.7.0",
    "protractor": "3.3.0",
    "tap-browser-color": "^0.1.2",
    "ts-node": "0.5.5",
